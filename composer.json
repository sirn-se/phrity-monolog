    "description": "Extensions for Monolog logger framework; Context normalizer, persister, interpolator.",
    "homepage": "https://phrity.sirn.se/monolog",
    "keywords": ["Logger", "Monolog", "PSR-3"],
    "license": "MIT",
    "authors": [
        {
            "name": "SÃ¶ren Jensen",
            "email": "sirn@sirn.se",
            "homepage": "https://phrity.sirn.se"
        }
    ],
    "autoload": {
        "psr-4": {
            "Phrity\\Monolog\\": "src/"
        }
